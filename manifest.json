{
  "manifest_version": 3,
  "name": "Sillyz Sidebear",
  "version": "1.0",
  "description": "Sidebear is a friend that helps process information anywhere.",
  "icons": {
    "16": "dev/src/public/images/16x16.png"
  },
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "permissions": ["sidePanel"]
}
